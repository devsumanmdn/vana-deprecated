{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "env": {
    "node": false,
    "browser": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/no-unused-prop-types": [2, { "skipShapeProps": true }],
    "jsx-a11y/anchor-is-valid": [
      0,
      {
        "aspects": ["noHref"]
      }
    ]
  },
  "overrides": [
    {
      "files": ["siteRouter.js"],
      "env": {
        "node": true,
        "browser": false
      }
    }
  ]
}
